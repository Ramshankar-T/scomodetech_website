<script type="text/javascript">  
	function validate(){  
		var name=document.myform.name.value;  
		var x=document.myform.email.value;  
		var atposition=x.indexOf("@");  
		var dotposition=x.lastIndexOf(".");	  
		var num=document.myform.phone.value;
		var company=document.myform.company.value; 
		var desg=document.myform.designation.value; 
		var status=false;
		
	if(name==""){  
		document.getElementById("namelocation").innerHTML= " <img src='img/close_red.png'/> Please Enter Your Name";  
		status=false;
	}else{  
		document.getElementById("namelocation").innerHTML=" <img src='img/add.png'/>";  
		status=true;
	}  
	  
	if (atposition<1 || dotposition<atposition+2 || dotposition+2>=x.length){   
		document.getElementById("passwordlocation").innerHTML=" <img src='img/close_red.png'/> Please Enter Valid Email Address ";  
		status=false; 
	}else{  
		document.getElementById("passwordlocation").innerHTML=" <img src='img/add.png'/>";  
	}  
	 if(num==""){  
		document.getElementById("numlocation").innerHTML= " <img src='img/close_red.png'/> Please Enter Your Mobile Number ";  
		status=false;
	}else{  
		document.getElementById("numlocation").innerHTML=" <img src='img/add.png'/>";  
		status=true;
	}	
	if(company==""){  
		document.getElementById("comlocation").innerHTML= " <img src='img/close_red.png'/> Please Enter Your Company Name";  
		status=false;
	}else{  
		document.getElementById("comlocation").innerHTML=" <img src='img/add.png'/>";  
		
	}
	if(desg==""){  
		document.getElementById("desglocation").innerHTML= " <img src='img/close_red.png'/> Please Enter Your Designation Name";  
		status=false;
	}else{  
		document.getElementById("desglocation").innerHTML=" <img src='img/add.png'/>";  
		
	}	
		return status;  
	}  
	
	$("#phone").intlTelInput({
      // allowDropdown: false,
